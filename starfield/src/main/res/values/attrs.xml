<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DownloadButton">
        <attr name="downloadIcon" format="reference"/>
        <attr name="activeThemeIcon" format="reference"/>
    </declare-styleable>
    <declare-styleable name="Slider">
        <attr name="title" format="string"/>
        <attr name="leftIcon" format="reference"/>
        <attr name="rightIcon" format="reference"/>
        <attr name="minValue" format="float"/>
        <attr name="maxValue" format="float"/>
        <attr name="stepSize" format="float"/>
        <attr name="value" format="float"/>
    </declare-styleable>
</resources>